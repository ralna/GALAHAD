#  book keeping

clean:
	@printf ' %-9s\t\t' "Cleaning"
	$(RM) $(LGS) $(LGD) $(LGQ) $(LGS64) $(LGD64) $(LGQ64) \
                            $(OBJS)/runlancelot_sif_$(PRECIS).o \
                            $(OBJD)/runlancelot_sif_$(PRECIS).o \
                            $(OBJQ)/runlancelot_sif_$(PRECIS).o \
                            $(OBJS64)/runlancelot_sif_$(PRECIS).o \
                            $(OBJD64)/runlancelot_sif_$(PRECIS).o \
                            $(OBJQ64)/runlancelot_sif_$(PRECIS).o
	@printf '[ OK ]\n'

cleanall:
	@printf ' %-14s\t\t' "Removing all $(PRECIS) precision object and module files"
	$(RM) -r $(OBJ)/* $(MOD)/*
	@printf '[ OK ]\n'
