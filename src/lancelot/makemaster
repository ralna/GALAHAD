                          $(OBJQ64)/runlancelot_sif_quadruple_64.o
	echo ""
	echo " Testing SIF interface to LANCELOT B"
	cd $(GALAHAD)/examples ; sdgal $(VERSION) lancelot -q ALLINITC
	echo ""
	echo " Tests for LANCELOT B complete"

