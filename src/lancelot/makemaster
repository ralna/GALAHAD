lancelotss: $(OBJECTSS) $(OBJS)/lancelots_single.o
	$(FORTRAN) $(RUNFFLAGS) $(SPECIAL) -o run_lancelot \
            $(OBJS)/lancelots_single.o -L$(OBJS) $(LIBS)
	- ./run_lancelot > lancelotss.output 2>&1
	cat lancelotss.output
	rm ./run_lancelot

