	$(RANLIB) $(LGS)
lancelotb_steering_double: $(OBJECTSD)
	$(RANLIB) $(LGD)
lancelotb_steering_quadruple: $(OBJECTSQ)
	$(RANLIB) $(LGQ)
lancelotb_steering_single_64: $(OBJECTSS64)
	$(RANLIB) $(LGS64)
lancelotb_steering_double_64: $(OBJECTSD64)
	$(RANLIB) $(LGD64)
lancelotb_steering_quadruple_64: $(OBJECTSQ64)
	$(RANLIB) $(LGQ64)

