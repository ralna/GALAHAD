	( cd ../qp ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                     qp_silent PRECIS=double PWD=$(PWD)/../qp )
	@printf ' %-9s %-15s\t\t' "Compiling" "galahad_qp"
	$(CP) galahad_qp.F90 galahad_qp1.F90
	$(MEX) galahad_qp1.F90 -output galahad_qp \
          -L$(OBJD) $(LIBS)  $(MFLAGS)
	$(RM) galahad_qp1.F90
	@printf '[ OK ]\n'

