#  book keeping

clean:
	@printf ' %-9s\t\t' "Cleaning generated mex files"
	$(RM) $(MLG) ./*.mexglx ./*.mexa64 $(OBJ)/libgalahad_matlab.a \
          $(MOD)/*matlab*
	@printf '[ OK ]\n'

cleanall:
	@printf ' %-14s\t\t' "Removing all $(PRECIS) precision object and module files"
	$(RM) -r $(OBJ)/* $(MOD)/* ./*.mexglx ./output_* ./*.mexa64
	@printf '[ OK ]\n'
