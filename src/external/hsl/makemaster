                   $(FORTRAN) $(FFLAGSN) hsl_zb01$(HSL_PRECIS).f90 )
	cd $(OBJ); $(HARR) hsl_zb01$(HSL_PRECIS).o; \
          $(RM) hsl_zb01$(HSL_PRECIS).f90 hsl_zb01$(HSL_PRECIS).o
	$(HRMARFILE) hsl_ma48$(HSL_PRECIS).o
	$(MVMODS)
	$(RANLIB) $(LH)
	@printf '[ OK ]\n'

hsl_zb01i.o: $(LH)(hsl_zb01$(HSL_IPRECIS).o)

$(LH)(hsl_zb01$(HSL_IPRECIS).o): $(HSLSRC)/hsl_zb01/hsl_zb01i.f90
	@printf ' %-9s %-15s\t\t' "Compiling" "hsl_zb01i"
	$(CP) $(HSLSRC)/hsl_zb01/hsl_zb01i.f90 \
           $(OBJ)/hsl_zb01$(HSL_IPRECIS).f90
	cd $(OBJ); $(FORTRAN) $(FFLAGS) hsl_zb01$(HSL_IPRECIS).f90 \
                || ( printf ' %-26s' "=> Disabling optimization " ; \
                   $(FORTRAN) $(FFLAGSN) hsl_zb01$(HSL_IPRECIS).f90 )
	cd $(OBJ); $(HARR) hsl_zb01$(HSL_IPRECIS).o; \
          $(RM) hsl_zb01$(HSL_IPRECIS).f90 hsl_zb01$(HSL_IPRECIS).o
	$(HRMARFILE) hsl_mc68$(HSL_IPRECIS).o
	$(MVMODS)
	$(RANLIB) $(LH)
	@printf '[ OK ]\n'

#  hsl_zd11

