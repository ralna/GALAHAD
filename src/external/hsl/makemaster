#  HSL C interfaces

hsl_ciface: hsl_$(PRECIS) hsl_ciface_$(PRECIS)
	@printf ' %-21s\n' "HSL C routines ($(PRECIS) $(SUCC)"
hsl_ciface_silent: hsl_$(PRECIS) hsl_ciface_$(PRECIS)
hsl_ciface_single: $(LHCS)(hsl_ma48s_ciface.o) \
                   $(LHCS)(hsl_ma57s_ciface.o) \
                   $(LHCS)(hsl_ma77s_ciface.o) \
                   $(LHCS)(hsl_ma86s_ciface.o) \
                   $(LHCS)(hsl_ma87s_ciface.o) \
                   $(LHCS)(hsl_ma97s_ciface.o) \
                   $(LHCS)(hsl_mc64s_ciface.o) \
                   $(LHCS)(hsl_mc68i_ciface.o) \
                   $(LHCS)(hsl_mi20s_ciface.o) \
                   $(LHCS)(hsl_mi28s_ciface.o)
hsl_ciface_double: $(LHCD)(hsl_ma48d_ciface.o) \
                   $(LHCD)(hsl_ma57d_ciface.o) \
                   $(LHCD)(hsl_ma77d_ciface.o) \
                   $(LHCD)(hsl_ma86d_ciface.o) \
                   $(LHCD)(hsl_ma87d_ciface.o) \
                   $(LHCD)(hsl_ma97d_ciface.o) \
                   $(LHCD)(hsl_mc64d_ciface.o) \
                   $(LHCD)(hsl_mc68i_ciface.o) \
                   $(LHCD)(hsl_mi20d_ciface.o) \
                   $(LHCD)(hsl_mi28d_ciface.o)
hsl_ciface_single_64: $(LHCS64)(hsl_ma48s_64_ciface.o) \
                      $(LHCS64)(hsl_ma57s_64_ciface.o) \
                      $(LHCS64)(hsl_ma77s_64_ciface.o) \
                      $(LHCS64)(hsl_ma86s_64_ciface.o) \
                      $(LHCS64)(hsl_ma87s_64_ciface.o) \
                      $(LHCS64)(hsl_ma97s_64_ciface.o) \
                      $(LHCS64)(hsl_mc64s_64_ciface.o) \
                      $(LHCS64)(hsl_mc68i_64_ciface.o) \
                      $(LHCS64)(hsl_mi20s_64_ciface.o) \
                      $(LHCS64)(hsl_mi28s_64_ciface.o)
hsl_ciface_double_64: $(LHCD64)(hsl_ma48d_64_ciface.o) \
                      $(LHCD64)(hsl_ma57d_64_ciface.o) \
                      $(LHCD64)(hsl_ma77d_64_ciface.o) \
                      $(LHCD64)(hsl_ma86d_64_ciface.o) \
                      $(LHCD64)(hsl_ma87d_64_ciface.o) \
                      $(LHCD64)(hsl_ma97d_64_ciface.o) \
                      $(LHCD64)(hsl_mc64d_64_ciface.o) \
                      $(LHCD64)(hsl_mc68i_64_ciface.o) \
                      $(LHCD64)(hsl_mi20d_64_ciface.o) \
                      $(LHCD64)(hsl_mi28d_64_ciface.o)

#  specific main packages

#  hsl_ad02

hsl_ad02: hsl_ad02_$(PRECIS)
	@printf ' %-21s\n' "HSL AD02 routines ($(PRECIS) $(SUCC)"
hsl_ad02_silent: hsl_ad02_$(PRECIS)
hsl_ad02_single: kinds $(LHS)(hsl_ad02s.o)
hsl_ad02_double: kinds $(LHD)(hsl_ad02d.o)
hsl_ad02_single_64: kinds $(LHS64)(hsl_ad02s_64.o)
hsl_ad02_double_64: kinds $(LHD64)(hsl_ad02d_64.o)

#  la04

la04: la04_$(PRECIS)
	@printf ' %-21s\n' "HSL LA04 routines ($(PRECIS) $(SUCC)"
la04_silent: la04_$(PRECIS)
la04_single: kinds $(LHS)(fa14s.o) $(LHS)(fd15s.o) $(LHS)(mc29s.o) \
             $(LHS)(mc59s.o) $(LHS)(la15s.o) $(LHS)(la04s.o)
la04_double: kinds $(LHD)(fa14d.o) $(LHD)(fd15d.o) $(LHD)(mc29d.o) \
             $(LHD)(mc59d.o) $(LHD)(la15d.o) $(LHD)(la04d.o)
la04_single_64: kinds $(LHS64)(fa14s_64.o) $(LHS64)(fd15s_64.o) \
                $(LHS64)(mc29s_64.o) $(LHS64)(mc59s_64.o) \
                $(LHS64)(la15s_64.o) $(LHS64)(la04s_64.o)
la04_double_64: kinds $(LHD64)(fa14d_64.o) $(LHD64)(fd15d_64.o) \
                $(LHD64)(mc29d_64.o) $(LHD64)(mc59d_64.o) \
                $(LHD64)(la15d_64.o) $(LHD64)(la04d_64.o)

