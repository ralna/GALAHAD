	cd $(OBJ) ; $(CC) -o hsl_ma77cs_$(PRECIS).o \
                      $(CFLAGS) hsl_ma77cs.c -I $(OBJ)
	$(RM) $(OBJ)//hsl_ma77cs.c
	$(FORTRAN) $(RUNFFLAGSOMP) $(SPECIAL) \
          $(NOFMAIN) -o run_hsl_ma77 $(OBJ)/hsl_ma77cs_$(PRECIS).o $(CLIBS)
	$(RM) ./factor_integer ./factor_real ./work_real ./temp1 ./delay_real
	- ./run_hsl_ma77 < ../test/hsl_ma77/C/hsl_ma77rs.data \
                         > ../test/hsl_ma77/C/hsl_ma77cs_$(PRECIS).output 2>&1
	$(CAT) ../test/hsl_ma77/C/hsl_ma77cs_$(PRECIS).output
	[ ! -f ./hsl_ma77rt.capture ] || \
          $(MV) ./hsl_ma77rt.capture ../test/hsl_ma77/
	$(RM) ./run_hsl_ma77
	@printf '[ OK ]\n'


