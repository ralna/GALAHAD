#  Main body of the installation makefile for the HSL subset packages

#  The HSL subset comprises packages that are used optionally by
#  a variey of optimization solvers/libraries such as IPOPT and GALAHAD.
#  Subset codes are templated, via C preprocessing commands, to support
#  multiple real, integer and logical types. The subset provides a
#  variety of linear-equation and linear-programming solvers, matrix
#  manipulation algorithms, and code automatic-differentiation utilities.
#  Test programs are provided for each package, and these use additional
#  HSL codes

#  The packages were developed by members of the Computational Mathematics
#  Group at the STFC-Rutherford Appleton Laboratory in the UK, and are
#  subject to licencing conditions. See the LICENCE file (LICENCE.archive
#  for the HSL Archive subset)  included as part of the distributed software.
#  as part of the distributed software.

#  Nick Gould, for GALAHAD productions
#  This version: 2024-03-27

#  include standard HSL subset makefile definitions

include $(GALAHAD)/src/makedefs/hsl_subset_definitions

#  ===================
#  compilation agendas
#  ===================

#  all packages

hsl_all: hsl hsl_ciface
hsl_all_silent: hsl_silent hsl_ciface_silent

#  all fortran packages

hsl: hsl_$(PRECIS)
	@printf ' %-21s\n' "HSL routines ($(PRECIS) $(SUCC)"
hsl_silent: hsl_$(PRECIS)
hsl_single: hsl_galahad_single
hsl_double: hsl_galahad_double
hsl_single_64: hsl_galahad_single_64
hsl_double_64: hsl_galahad_double_64

#  HSL packages for LANCELOT

hsl_lancelot: hsl_lancelot_$(PRECIS)
	@printf ' %-21s\n' "HSL LANCELOT routines ($(PRECIS) $(SUCC)"
hsl_lancelot_silent: hsl_lancelot_$(PRECIS)
hsl_lancelot_single: kinds $(LHS)(ma27s.o) metis_silent \
                     $(LHS)(hsl_zd11s.o) $(LHS)(mc71s.o) $(LHS)(ma57s.o) \
                     $(LHS)(mc21s.o) $(LHS)(mc34s.o) $(LHS)(mc47s.o) \
                     $(LHS)(mc59s.o) $(LHS)(mc64s.o) $(LHS)(hsl_ma57s.o) \
                     $(LHS)(ma61s.o) $(LHS)(hsl_ad02s.o)
hsl_lancelot_double: kinds $(LHD)(ma27d.o) metis_silent \
                     $(LHD)(hsl_zd11d.o) $(LHD)(mc71d.o) $(LHD)(ma57d.o) \
                     $(LHD)(mc21d.o) $(LHD)(mc34d.o) $(LHD)(mc47d.o) \
                     $(LHD)(mc59d.o) $(LHD)(mc64d.o) $(LHD)(hsl_ma57d.o) \
                     $(LHD)(ma61d.o) $(LHD)(hsl_ad02d.o)
hsl_lancelot_single_64: kinds $(LHS64)(ma27s_64.o) metis_silent \
                        $(LHS64)(hsl_zd11s_64.o) \
                        $(LHS64)(mc71s_64.o) $(LHS64)(ma57s_64.o) \
                        $(LHS64)(mc21s_64.o) $(LHS64)(mc34s_64.o) \
                        $(LHS64)(mc47s_64.o) $(LHS64)(mc59s_64.o) \
                        $(LHS64)(mc64s_64.o) $(LHS64)(hsl_ma57s_64.o) \
                        $(LHS64)(ma61s_64.o) $(LHS64)(hsl_ad02s_64.o)
hsl_lancelot_double_64: kinds $(LHD64)(ma27d_64.o) metis_silent \
                        $(LHD64)(hsl_zd11d_64.o) \
                        $(LHD64)(mc71d_64.o) $(LHD64)(ma57d_64.o) \
                        $(LHD64)(mc21d_64.o) $(LHD64)(mc34d_64.o) \
                        $(LHD64)(mc47d_64.o) $(LHD64)(mc59d_64.o) \
                        $(LHD64)(mc64d_64.o) $(LHD64)(hsl_ma57d_64.o) \
                        $(LHD64)(ma61d_64.o) $(LHD64)(hsl_ad02d_64.o)

#  HSL packages for GALAHAD

hsl_galahad: hsl_galahad_$(PRECIS)
	@printf ' %-21s\n' "HSL GALAHAD routines ($(PRECIS) $(SUCC)"
hsl_galahad_silent: hsl_galahad_$(PRECIS)
hsl_galahad_single: hsl_lancelot_single \
                    $(LHS)(fa14s.o) $(LHS)(fd15s.o) $(LHS)(kb07i.o) \
                    $(LHS)(kb07s.o) $(LHS)(mc13s.o) $(LHS)(mc19s.o) \
                    $(LHS)(mc20s.o) $(LHS)(mc22s.o) $(LHS)(mc23s.o) \
                    $(LHS)(mc24s.o) $(LHS)(mc29s.o) $(LHS)(mc30s.o) \
                    $(LHS)(mc60s.o) $(LHS)(mc61s.o) $(LHS)(mc77s.o) \
                    $(LHS)(la15s.o) $(LHS)(la04s.o) $(LHS)(ma27s.o) \
                    $(LHS)(ma28s.o) $(LHS)(ma30s.o) $(LHS)(ma33s.o) \
                    $(LHS)(mi21s.o) $(LHS)(mi24s.o) $(LHS)(mi26s.o) \
                    $(LHS)(hsl_zb01i.o) $(LHS)(hsl_of01i.o) \
                    $(LHS)(hsl_zb01s.o) $(LHS)(hsl_kb22l.o) \
                    $(LHS)(hsl_mc34s.o) $(LHS)(hsl_of01s.o) \
                    $(LHS)(hsl_mc65s.o) $(LHS)(hsl_mc68i.o) \
                    $(LHS)(hsl_mc69s.o) $(LHS)(hsl_mc64s.o) \
                    $(LHS)(hsl_mc78i.o) $(LHS)(hsl_mc80s.o) \
                    $(LHS)(hsl_ma48s.o) $(LHS)(hsl_ma54s.o) \
                    $(LHS)(hsl_ma64s.o) $(LHS)(hsl_ma77s.o) \
                    $(LHS)(hsl_ma86s.o) $(LHS)(hsl_ma87s.o) \
                    $(LHS)(hsl_ma97s.o) $(LHS)(hsl_mi32s.o) \
                    $(LHS)(hsl_mi20s.o) $(LHS)(hsl_mi28s.o) \
                    $(LHS)(hsl_mi35s.o)

hsl_galahad_double: hsl_lancelot_double \
                    $(LHD)(fa14d.o) $(LHD)(fd15d.o) $(LHD)(kb07i.o) \
                    $(LHD)(kb07d.o) $(LHD)(mc13d.o) $(LHD)(mc19d.o) \
                    $(LHD)(mc20d.o) $(LHD)(mc22d.o) $(LHD)(mc23d.o) \
                    $(LHD)(mc24d.o) $(LHD)(mc29d.o) $(LHD)(mc30d.o) \
                    $(LHD)(mc60d.o) $(LHD)(mc61d.o) $(LHD)(mc77d.o) \
                    $(LHD)(la15d.o) $(LHD)(la04d.o) $(LHD)(ma27d.o) \
                    $(LHD)(ma28d.o) $(LHD)(ma30d.o) $(LHD)(ma33d.o) \
                    $(LHD)(mi21d.o) $(LHD)(mi24d.o) $(LHD)(mi26d.o) \
                    $(LHD)(hsl_zb01i.o) $(LHD)(hsl_of01i.o) \
                    $(LHD)(hsl_zb01d.o) $(LHD)(hsl_kb22l.o) \
                    $(LHD)(hsl_mc34d.o) $(LHD)(hsl_of01d.o) \
                    $(LHD)(hsl_mc65d.o) $(LHD)(hsl_mc68i.o) \
                    $(LHD)(hsl_mc69d.o) $(LHD)(hsl_mc64d.o) \
                    $(LHD)(hsl_mc78i.o) $(LHD)(hsl_mc80d.o) \
                    $(LHD)(hsl_ma48d.o) $(LHD)(hsl_ma54d.o) \
                    $(LHD)(hsl_ma64d.o) $(LHD)(hsl_ma77d.o) \
                    $(LHD)(hsl_ma86d.o) $(LHD)(hsl_ma87d.o) \
                    $(LHD)(hsl_ma97d.o) $(LHD)(hsl_mi32d.o) \
                    $(LHD)(hsl_mi20d.o) $(LHD)(hsl_mi28d.o) \
                    $(LHD)(hsl_mi35d.o)
