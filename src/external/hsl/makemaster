	echo " Exhaustive test of subroutine interface to hsl_zb01i"
	$(CP) ../test/hsl_zb01/hsl_zb01it.f90 $(OBJ)/hsl_zb01it.f90
	cd $(OBJ) ; $(FORTRAN) -o hsl_zb01it_$(PRECIS).o \
          $(FFLAGSS) hsl_zb01it.f90
	$(RM) $(OBJ)/hsl_zb01it.f90
	$(FORTRAN) $(RUNFFLAGSOMP) $(SPECIAL) \
           -o run_hsl_zb01i $(OBJ)/hsl_zb01it_$(PRECIS).o $(LIBS)
	- ./run_hsl_zb01i > ../test/hsl_zb01/hsl_zb01it_$(PRECIS).output 2>&1
	$(CAT) ../test/hsl_zb01/hsl_zb01it_$(PRECIS).output
	$(RM) ./run_hsl_zb01i $(OBJ)/hsl_zb01it_$(PRECIS).o
	@printf '[ OK ]\n'

#  hsl_zd11

hsl_zd11t: hsl_zd11.o ../test/hsl_zd11/hsl_zd11rt.f90
	echo " Exhaustive test of subroutine interface to hsl_zd11"
	$(CP) ../test/hsl_zd11/hsl_zd11rt.f90 $(OBJ)/hsl_zd11rt.f90
	cd $(OBJ) ; $(FORTRAN) -o hsl_zd11t_$(PRECIS).o \
          $(FFLAGSS) hsl_zd11rt.f90
	$(RM) $(OBJ)/hsl_zd11rt.f90
	$(FORTRAN) $(RUNFFLAGSOMP) $(SPECIAL) \
           -o run_hsl_zd11 $(OBJ)/hsl_zd11t_$(PRECIS).o $(LIBS)
	- ./run_hsl_zd11 > ../test/hsl_zd11/hsl_zd11t_$(PRECIS).output 2>&1
	$(CAT) ../test/hsl_zd11/hsl_zd11t_$(PRECIS).output
	$(RM) ./run_hsl_zd11 $(OBJ)/hsl_zd11t_$(PRECIS).o
	@printf '[ OK ]\n'

#  ============
#  book keeping
#  ============

clean:
	@printf ' %-9s\t\t' "Removing $(package) files"
	$(HRMARFILE) $(package)_$(PRECIS).o
	$(RM) $(OBJ)/$(package)t_$(PRECIS).o $(OBJ)/$(package)s_$(PRECIS).o
	@printf '[ OK ]\n'

cleanall:
	@printf ' %-14s\t\t' \
          "Removing all $(PRECIS) precision object and module files"
	$(RM) -r $(OBJ)/* $(MOD)/*
	@printf '[ OK ]\n'

cleanallc:
	@printf ' %-9s\t' \
          "Removing all $(PRECIS) precision C-interface object and module files"
	$(RM) $(OBJ)/libhsl_c.a $(MOD)/*ciface.mod
	@printf '[ OK ]\n'

names:
	@echo "HSLSUBSET: $(HSLSUBSET)"
	@echo "HSLARCHIVESUBSET: $(HSLARCHIVESUBSET)"
