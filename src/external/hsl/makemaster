           -o run_hsl_ma86 $(OBJ)/hsl_ma86t_$(PRECIS).o $(LIBS)
	./run_hsl_ma86
	- ./run_hsl_ma86 > ../test/hsl_ma86/hsl_ma86t_$(PRECIS).output 2>&1
	$(CAT) ../test/hsl_ma86/hsl_ma86t_$(PRECIS).output
	$(MV) ./hsl_ma86*.capture ../test/hsl_ma86/
#	$(RM) ./run_hsl_ma86 $(OBJ)/hsl_ma86t_$(PRECIS).o
	@printf '[ OK ]\n'

