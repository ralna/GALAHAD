                   $(FORTRAN) $(FFLAGSN) hsl_metis$(HSL_64).f90 )
	cd $(OBJ); $(HARR) hsl_metis$(HSL_64).o; \
          $(RM) hsl_metis$(HSL_64).f90 hsl_metis$(HSL_64).o
	$(HRMARFILE) ma57$(HSL_PRECIS).o
