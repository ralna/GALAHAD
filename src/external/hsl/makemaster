#  all fortran packages

hsl_test: hsl fa04t fa14t fd15t kb07t kb07it la04t la15t ma27t ma28t ma30t \
          ma33t ma57t ma61t mc13t mc19t mc20t mc21t mc22t mc23t mc24t \
          mc29t mc30t mc34t mc37t mc47t mc49t mc54t mc57t mc59t mc60t \
          mc61t mc64t mc71t mc77t mi21t mi24t mi26t ym01t ym11t \
          hsl_ad02t hsl_fa14t hsl_kb22t hsl_ma48t hsl_ma54t hsl_ma57t \
          hsl_ma64t hsl_ma77t hsl_ma86t hsl_ma87t hsl_ma97t hsl_mc34t \
          hsl_mc64t hsl_mc65t hsl_mc68t hsl_mc69t hsl_mc78t hsl_mc80t \
          hsl_mi20t hsl_mi28t hsl_mi32t hsl_mi35t hsl_of01t hsl_of01it \
          hsl_zb01t hsl_zb01it hsl_zd11t

#  all C interface packages

hsl_ciface_test: hsl_ciface hsl_ma48cs hsl_ma57cs hsl_ma77cs \
                 hsl_ma86cs hsl_ma87cs hsl_ma97cs hsl_mc64cs \
                 hsl_mc68cs hsl_mi20cs hsl_mi28cs

#  fa04

fa04t:  fa04.o ../test/fa04/fa04rt.f
	echo " Exhaustive test of subroutine interface to fa04"
	$(CP) ../test/fa04/fa04rt.f $(OBJ)/fa04rt.f90
	cd $(OBJ) ; $(FORTRAN) -o fa04t_$(PRECIS).o $(FFLAGS77S) fa04rt.f90
	$(RM) $(OBJ)/fa04rt.f90
	$(FORTRAN) $(RUNFFLAGSOMP) $(SPECIAL) \
           -o run_fa04 $(OBJ)/fa04t_$(PRECIS).o $(LIBS)
	$(RMOOC)
	- ./run_fa04 > ../test/fa04/fa04t_$(PRECIS).output 2>&1
	$(CAT) ../test/fa04/fa04t_$(PRECIS).output
	$(RM) ./run_fa04 $(OBJ)/fa04t_$(PRECIS).o
	@printf '[ OK ]\n'

