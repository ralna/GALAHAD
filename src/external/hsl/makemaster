hsl_ma77t: hsl_ma77 hsl_fa14.o mc47.o mc34.o ym11.o mc54.o \
          ../test/hsl_ma77/hsl_ma77rt.f90
	echo " Exhaustive test of subroutine interface to hsl_ma77"
	$(CP) ../test/hsl_ma77/hsl_ma77rt.f90 $(OBJ)/hsl_ma77rt.f90
	cd $(OBJ) ; $(FORTRAN) -o hsl_ma77t_$(PRECIS).o \
          $(FFLAGSS) hsl_ma77rt.f90
	$(RM) $(OBJ)/hsl_ma77rt.f90
	$(FORTRAN) $(RUNFFLAGSOMP) $(SPECIAL) \
           -o run_hsl_ma77 $(OBJ)/hsl_ma77t_$(PRECIS).o $(LIBS)
	$(RMOOC)
	- ./run_hsl_ma77 > ../test/hsl_ma77/hsl_ma77t_$(PRECIS).output 2>&1
	$(CAT) ../test/hsl_ma77/hsl_ma77t_$(PRECIS).output
	[ ! -f ./hsl_ma77rt.capture ] || \
          $(MV) ./hsl_ma77rt.capture ../test/hsl_ma77/
	$(RMOOC)
	$(RM) ./run_hsl_ma77 $(OBJ)/hsl_ma77t_$(PRECIS).o
	@printf '[ OK ]\n'

hsl_ma77cs: ../test/hsl_ma77/C/hsl_ma77rs.c
	echo " Exhaustive test of C function interface to hsl_ma77"
