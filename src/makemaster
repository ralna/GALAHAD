#  -------------------------------
#  everything with AMPL not CUTEst
#  -------------------------------

all_ampl: all_ampl_$(PRECIS)

all_ampl_single:
	( cd ampl ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                    galahad_ampl_single $(S32) PWD=$(PWD)/ampl \
                      AMPLDIR=$(AMPLDIR) \
                      CUSTOM_AMPL_FLAGS="-DQP -DCQP -DQPA -DQPB -DQPC \
                                         -DPRESOLVE -DFILTRANE -DLANCELOT" )

all_ampl_double:
	( cd ampl ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                    galahad_ampl_double $(D32) PWD=$(PWD)/ampl \
                      AMPLDIR=$(AMPLDIR) \
                      CUSTOM_AMPL_FLAGS="-DQP -DCQP -DQPA -DQPB -DQPC \
                                         -DPRESOLVE -DFILTRANE -DLANCELOT" )

all_ampl_quadruple:
	( cd ampl ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                    galahad_ampl_double $(Q32) PWD=$(PWD)/ampl \
                      AMPLDIR=$(AMPLDIR) \
                      CUSTOM_AMPL_FLAGS="-DQP -DCQP -DQPA -DQPB -DQPC \
                                         -DPRESOLVE -DFILTRANE -DLANCELOT" )

all_ampl_single_64:
	( cd ampl ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                    galahad_ampl_single $(S32)_64 PWD=$(PWD)/ampl \
                      AMPLDIR=$(AMPLDIR) \
                      CUSTOM_AMPL_FLAGS="-DQP -DCQP -DQPA -DQPB -DQPC \
                                         -DPRESOLVE -DFILTRANE -DLANCELOT" )

all_ampl_double_64:
	( cd ampl ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                    galahad_ampl_double $(D32)_64 PWD=$(PWD)/ampl \
                      AMPLDIR=$(AMPLDIR) \
                      CUSTOM_AMPL_FLAGS="-DQP -DCQP -DQPA -DQPB -DQPC \
                                         -DPRESOLVE -DFILTRANE -DLANCELOT" )

all_ampl_quadruple_64:
	( cd ampl ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                    galahad_ampl_double $(Q32)_64 PWD=$(PWD)/ampl \
                      AMPLDIR=$(AMPLDIR) \
                      CUSTOM_AMPL_FLAGS="-DQP -DCQP -DQPA -DQPB -DQPC \
                                         -DPRESOLVE -DFILTRANE -DLANCELOT" )

