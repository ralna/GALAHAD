	( cd lancelot ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
             lancelotb_sif $(S64) PWD=$(PWD)/lancelot )
lancelotb_sif_double_64:
	( cd lancelot ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
            lancelotb_sif $(D64) PWD=$(PWD)/lancelot )
