#  ----------
#  LANCELOT B
#  ----------

lancelotb: lancelotb_$(PRECIS)

lancelotb_single:
	( cd lancelot ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) lancelotb \
                      $(S32) PWD=$(PWD)/lancelot )
lancelotb_double:
	( cd lancelot ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) lancelotb \
                      $(D32) PWD=$(PWD)/lancelot )
lancelotb_quadruple:
	( cd lancelot ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) lancelotb \
                      $(Q32) PWD=$(PWD)/lancelot )
lancelotb_single_64:
	( cd lancelot ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) lancelotb \
                      $(S64) PWD=$(PWD)/lancelot )
lancelotb_double_64:
	( cd lancelot ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) lancelotb \
                      $(D64) PWD=$(PWD)/lancelot )
lancelotb_quadruple_64:
	( cd lancelot ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) lancelotb \
                      $(Q64) PWD=$(PWD)/lancelot )

