#  Main body of the installation makefile for the GALAHAD library

#  N. Gould, D. Orban and Ph. L. Toint.
#  This version: 2024-11-23

SHELL = /bin/$(BINSHELL)

#  shorthands for real precision and integer lengths control

S32 = PRECIS=single
D32 = PRECIS=double
Q32 = PRECIS=quadruple
S64 = PRECIS=single_64 INTEGER=64bit
D64 = PRECIS=double_64 INTEGER=64bit
Q64 = PRECIS=quadruple_64 INTEGER=64bit

#  main compilations and runs

#  -------------------------------
#  everything with CUTEst not AMPL
#  -------------------------------

all_cutest: all_cutest_$(PRECIS)

all_cutest_single:
	( cd lancelot ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                          lancelotb_sif $(S32) PWD=$(PWD)/lancelot )
	( cd all_qp   ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                          all_cutest all_qplib $(S32) PWD=$(PWD)/all_qp )
	( cd all_rq   ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         all_cutest $(S32) PWD=$(PWD)/all_rq )
	( cd filtrane ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         filtrane_cutest $(S32) PWD=$(PWD)/filtrane )
	( cd wcp      ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         wcp_cutest $(S32) PWD=$(PWD)/wcp )
	( cd bsc      ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         bsc $(S32) PWD=$(PWD)/bsc )
	( cd fdh      ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         fdh_cutest $(S32) PWD=$(PWD)/fdh )
	( cd tru      ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         tru_cutest $(S32) PWD=$(PWD)/tru )
	( cd arc      ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         arc_cutest $(S32) PWD=$(PWD)/arc )
	( cd trb      ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         trb_cutest $(S32) PWD=$(PWD)/trb )
	( cd nls      ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         nls_cutest $(S32) PWD=$(PWD)/nls )
	( cd all_go   ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         all_cutest $(S32) PWD=$(PWD)/all_go )

all_cutest_double:
	( cd lancelot ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         lancelotb_sif $(D32) PWD=$(PWD)/lancelot )
	( cd all_qp   ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                         all_cutest all_qplib $(D32) PWD=$(PWD)/all_qp )
	( cd all_rq   ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                        all_cutest $(D32) PWD=$(PWD)/all_rq )
	( cd filtrane ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                        filtrane_cutest $(D32) PWD=$(PWD)/filtrane )
	( cd wcp      ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                          wcp_cutest $(D32) PWD=$(PWD)/wcp )
	( cd bsc      ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                          bsc $(D32) PWD=$(PWD)/bsc )
	( cd fdh      ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                          fdh_cutest $(D32) PWD=$(PWD)/fdh )
	( cd tru      ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
