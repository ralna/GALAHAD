	$(RANLIB) $(LGQ64)

#  book keeping

clean:
	@printf ' %-9s\t\t' "Removing $(package) files"
	$(RMARFILES) $(PACKAGES)
	@printf '[ OK ]\n'

cleanall:
	@printf ' %-14s\t\t' \
          "Removing all $(PRECIS) precision object and module files"
	$(RM) -r $(OBJ)/* $(MOD)/*
	@printf '[ OK ]\n'

#  include required package intermediaries

include $(GALAHAD)/src/makedefs/intermediaries

#  CUTEst specific interfaces

make_blls_cutest:
	( cd $(GALAHAD)/src/blls ; \
          $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) blls_cutest_addon \
          PRECIS=$(PRECIS) PWD=$(GALAHAD)/src/blls )

make_bllsb_cutest:
	( cd $(GALAHAD)/src/bllsb ; \
          $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) bllsb_cutest_addon \
          PRECIS=$(PRECIS) PWD=$(GALAHAD)/src/bllsb )

