	$(RANLIB) $(LGS64)
$(package)_cutest_double_64: $(DEPENDENCIES) $(CUTESTDEPS)
	$(RANLIB) $(LGD64)
$(package)_cutest_quadruple_64: $(DEPENDENCIES) $(CUTESTDEPS)
	$(RANLIB) $(LGQ64)

