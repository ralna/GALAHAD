               make_smt make_svt make_space make_sils make_hsl \
               make_copyright make_spralrb make_amd make_$(SSIDS)
make_sls_deps_single_64: make_sls_deps_single
make_sls_deps_double_64: make_sls_deps_double
make_sls_deps_quadruple_64: make_sls_deps_quadruple

