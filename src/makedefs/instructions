$(package)qti_64_valgrind: $($(PACKAGE)QTI64)
	echo ""
	echo " Exhaustive valgrind test of subroutine interface to $(package)"
	$(FORTRAN) $(RUNFFLAGSOMP) $(SPECIAL) \
          -o run_$(package) $(R$(PACKAGE)QTI64)
	$(RMOOC)
	- valgrind $(VALGRIND) ./run_$(package) > \
           $(package)qts_64__valgrind.output 2>&1
	cat $(package)qts_64_valgrind.output
	rm ./run_$(package)

$(package)qti_64_gdb: $($(PACKAGE)QTI64)
	echo ""
	echo " Exhaustive gdb test of subroutine interface to $(package)"
	$(FORTRAN) $(RUNFFLAGSOMP) $(SPECIAL) \
          -o run_$(package) $(R$(PACKAGE)QTI64)
	$(RMOOC)
	- gdb $(GDB) ./run_$(package)
	rm ./run_$(package)

