	@printf ' %-21s\n' "GALAHAD: $(PACKAGE) ($(PRECIS) $(SUCC_C)"
$(package)_ciface_single: $(package) $($(PACKAGE)OBJECTSCS)
	$(RANLIB) $(LGS)
	$(RANLIB) $(CLGS)
$(package)_ciface_double: $(package) $($(PACKAGE)OBJECTSCD)
	$(RANLIB) $(LGD)
	$(RANLIB) $(CLGD)
$(package)_ciface_quadruple: $(package) $($(PACKAGE)OBJECTSCQ)
	$(RANLIB) $(LGQ)
	$(RANLIB) $(CLGQ)
$(package)_ciface_single_64: $(package) $($(PACKAGE)OBJECTSCS64)
	$(RANLIB) $(LGS64)
	$(RANLIB) $(CLGS64)
$(package)_ciface_double_64: $(package) $($(PACKAGE)OBJECTSCD64)
	$(RANLIB) $(LGD64)
	$(RANLIB) $(CLGD64)
$(package)_ciface_quadruple_64: $(package) $($(PACKAGE)OBJECTSCQ64)
	$(RANLIB) $(LGQ64)
	$(RANLIB) $(CLGQ64)

