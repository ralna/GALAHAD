                   $(OBJQ64)/$(package)cti_quadruple_64.o
$(PACKAGE)CQTF64 = $($(PACKAGE)OBJECTSCQ64) $(TESTDEPS) \
                   $(OBJQ64)/$(package)ctf_quadruple_64.o

