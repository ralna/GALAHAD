	$(RANLIB) $(LGS64)
	$(RANLIB) $(CLGS64)
	$(RANLIB) $(PYLGS64)
$(package)_pyiface_double_64: $($(PACKAGE)OBJECTSPYD64)
	$(RANLIB) $(LGD64)
	$(RANLIB) $(CLGD64)
	$(RANLIB) $(PYLGD64)
