                      ldlt_nopiv.cxx )
	cd $(OBJ) ; $(RARR) ldlt_nopiv.o ; $(RM) -r ldlt_nopiv.cxx \
          ldlt_nopiv.o omp.hxx config.h config1.h ssids
	@printf '[ OK ]\n'

