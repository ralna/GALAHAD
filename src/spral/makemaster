                    $(FORTRAN) -o gpu_smalloc.o $(FFLAGSNOMP) gpu_smalloc.F90 )
	cd $(OBJ) ; $(RARR) gpu_smalloc.o ; $(RM) gpu_smalloc.F90 gpu_smalloc.o
